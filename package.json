{
  "name": "onesignal-api-client-core",
  "version": "1.2.1",
  "description": "Typescript client for OneSignal Server Rest API",
  "author": "kvandake",
  "license": "MIT",
  "keywords": [
    "onesignal",
    "typescript",
    "axios",
    "nodejs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kvandake/onesignal"
  },
  "bugs": {
    "url": "https://github.com/kvandake/onesignal/issues"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "build": "yarn run clean && tsc",
    "test": "jest",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "yarn run build",
    "prepublishOnly": "yarn test && yarn run lint",
    "preversion": "yarn run lint",
    "version": "yarn run format && git add -A",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "axios": "^0.27.2"
  },
  "devDependencies": {
    "@types/jest": "^28.1.7",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.8",
    "tslint": "^6.0.0-beta1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.7.4"
  },
  "files": [
    "lib/**/*"
  ]
}
